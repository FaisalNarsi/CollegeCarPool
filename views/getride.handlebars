													<div class="col-xs-12 col-sm-4 col-md-4 text-left">
														<div class="well emptywell">
														<p><strong>Cost: $</strong> 3</p>
														</div>
													</div>

													<div class="col-xs-12 col-sm-4 col-md-4 text-left">
														<div class="well emptywell">
															<a href="index.html" class="button alt center text-right">Book Now!</a>
														</div>
													</div>
												{{/each}}

										 </section>


			<!-- Three -->
				<section id="three" class="wrapper post bg-img" data-bg="banner4.jpg">
					<div class="inner">
						<article class="box">
							<header>
								<h2>KarPools at your University</h2>
							</header>

							<div class="well">
								<div class="row">
									<div class="row">
										<div class="col-xs-12">
											<div class="well emptywell">
												<h4>UT Austin to Downtown Houston, Texas</h4>
											</div>
										</div>
									</div>

								<div class="row">
									<div class="col-xs-4 col-sm-2 col-md-1">
										<img src="http://www.drodd.com/images12/facebook-profile-picture29.jpg" class="img responsive img-thumbnail full_image">
									</div>

									<div class="col-xs-8 col-sm-10 col-md-5 text-center">
										<div class="well emptywell">
											<h5 style="font-size:19px">Driver</h5>
										</div>
									</div>

									<div class="col-xs-4 col-sm-5 col-md-3 text-center">
										<div class="well emptywell">
											<strong>
												<span class="glyphicons glyphicons-user">
												</span>
												<span class="hidden xs">Seat Required: 1</span>
											</strong>
										</div>
									</div>

									<div class="col-xs-4 col-sm-5 col-md-3">
										<div class="well text-center emptywell">
											<span class="glyphicons glyphicons-gender-male">
											</span>
											<span class="hidden-xs">Male</span>
										</div>
									</div>

									<div class="row">
									</div>

									<div class="col-xs-12 col-sm-4 col-md-4 text-left">
										<div class="well emptywell">
											<p>Available Times: Wed Thu Fri Sat 4:30am - 7pm</p>
										</div>
									</div>
							</div>

							<footer>
								<center><a href="index.html" class="button alt">Meetup</a>
							</footer>
						</article>
					</div>
					<a href="#four" class="more">Meet Up!</a>
				</section>
